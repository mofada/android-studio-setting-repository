<templateSet group="CustomDart">
  <template name="fori" value="for(int i = 0; i &lt; $num$; i++){&#10;}" description="for" toReformat="false" toShortenFQNames="true">
    <variable name="num" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="DART" value="true" />
    </context>
  </template>
  <template name="forin" value="for (var $data$ in $colleaction$) {&#10;}" description="forin循环" toReformat="false" toShortenFQNames="true">
    <variable name="data" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="colleaction" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="DART" value="true" />
    </context>
  </template>
  <template name="forr" value="for(int i = $num$; i &gt; 0; i--){&#10;}" description="for循环" toReformat="false" toShortenFQNames="true">
    <variable name="num" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="DART" value="true" />
    </context>
  </template>
  <template name="sin" value="static final $className$ _singleton = $className$._instance();&#10;&#10;factory $className$() {&#10;  return _singleton;&#10;}&#10;&#10;$className$._instance();&#10;&#10;static $className$ getInstance() =&gt; _singleton;" description="dart singleton" toReformat="false" toShortenFQNames="true">
    <variable name="className" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="DART" value="true" />
    </context>
  </template>
</templateSet>